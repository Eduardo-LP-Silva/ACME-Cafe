{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-plusplus": "off",
        "no-await-in-loop": "off",
        "no-underscore-dangle": "off",
        "func-names": "off",
        "max-len": "off",
        "no-use-before-define": "off"
    }
}
